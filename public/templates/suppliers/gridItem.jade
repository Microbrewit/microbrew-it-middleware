.grid-result.search-grid-result
	a.content(href='suppliers/#{result.supplierId}')
		span(class='minibubble' style="display:inline-block;width:50px;height:50px;padding:0;margin:0;border-radius:50%;border:1px solid rgba(0,0,0,0.2);background: url('#{country.getImageByName(result.origin.name)}') center center;background-size: 152%;")
		h3 #{result.name}
		.type Supplier
		.meta
			.meta-table
				.meta-cell #{result.origin.name}